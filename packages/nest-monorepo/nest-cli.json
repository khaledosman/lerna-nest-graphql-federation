{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway-service/src",
  "monorepo": true,
  "root": "apps/gateway-service",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway-service/tsconfig.app.json"
  },
  "projects": {
    "comment-service": {
      "type": "application",
      "root": "apps/comment-service",
      "entryFile": "main",
      "sourceRoot": "apps/comment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comment-service/tsconfig.app.json"
      }
    },
    "article-service": {
      "type": "application",
      "root": "apps/article-service",
      "entryFile": "main",
      "sourceRoot": "apps/article-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/article-service/tsconfig.app.json"
      }
    },
    "gateway-service": {
      "type": "application",
      "root": "apps/gateway-service",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-service/tsconfig.app.json"
      }
    }
  }
}